{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "weather-api",
      "urls": ["https://api.open-meteo.com/**"],
      "cacheConfig": {
        "maxSize": 20,
        "maxAge": "30m",
        "strategy": "freshness",
        "timeout": "8s"
      }
    },
    {
      "name": "air-quality-api",
      "urls": ["https://air-quality-api.open-meteo.com/**"],
      "cacheConfig": {
        "maxSize": 10,
        "maxAge": "30m",
        "strategy": "freshness",
        "timeout": "8s"
      }
    },
    {
      "name": "geocoding-api",
      "urls": ["https://geocoding-api.open-meteo.com/**", "https://nominatim.openstreetmap.org/**"],
      "cacheConfig": {
        "maxSize": 50,
        "maxAge": "7d",
        "strategy": "freshness",
        "timeout": "5s"
      }
    },
    {
      "name": "archive-api",
      "urls": ["https://archive-api.open-meteo.com/**"],
      "cacheConfig": {
        "maxSize": 10,
        "maxAge": "1d",
        "strategy": "freshness",
        "timeout": "10s"
      }
    }
  ]
}
